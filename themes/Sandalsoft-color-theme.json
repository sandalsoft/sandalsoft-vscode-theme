    "widget.shadow": "#00000055",
    "gitDecoration.ignoredResourceForeground": "#535964",
    "gitDecoration.deletedResourceForeground": "#da0f00",
    "gitDecoration.modifiedResourceForeground": "#e5c07b",
    "gitDecoration.untrackedResourceForeground": "#98c379",
    "gitDecoration.conflictingResourceForeground": "#ff7300",
    "editorLineNumber.activeForeground": "#d1d5e488"
  },
  "tokenColors": [
    {
      "name": "Identifier",
      "scope": "variable, support.variable, support.class, support.constant, meta.definition.variable",
      "settings": {
        "foreground": "#7effff"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#9b9a9a"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#5d6572"
      }
    },
    {
      "name": "Keyword Operator",
      "scope": "keyword.operator, storage.type.function.arrow",
      "settings": {
        "foreground": "#ece9e9"
      }
    },
    {
      "name": "Regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#b863c0"
      }
    },
    {
      "name": "Regexp Anchor",
      "scope": "keyword.control.anchor.regexp, keyword.operator.quantifier.regexp",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#bd39c9"
      }
    },
    {
      "name": "String punctuation - Quotes and regex //",
      "scope": "punctuation.definition.string",
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#9e9e9e"
      }
    },
    //meta.brace.round
    {
      "name": "Code punctuation",
      "scope": "meta.brace.round", // punctuation.definition.block, punctuation.separator",
      "settings": {
        // "fontStyle": "bold ",
        "foreground": "#8df5f5"
      }
    },
    {
      "name": "Boolean",
      "scope": "constant.language.boolean",
      "settings": {
        // "fontStyle": "bold",
        "foreground": "#fded59"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        // "fontStyle": "bold",
        "foreground": "#d36051"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#7bd4d4"
      }
    },

    {
      //4d9eeb
      "name": "Keyword",
      "scope": "keyword, modifier, variable.language.this, support.type.object, constant.language",
      "settings": {
        "foreground": "#dbdada"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.type.class",
        "entity.name.class",
        "entity.name.module",
        "entity.name.type",
        "storage.identifier",
        "support.class"
      ],
      "settings": {
        "foreground": "#5fc441"
        // foreground": "#d6d84e"
      }
    },
    {
      "name": "Function call",
      "scope": "entity.name.function.js, entity.name.function.ts, support.function, meta.function-call",
      "settings": {
        // "fontStyle": "italic",
        "foreground": "#498ee9"
        // "foreground": "#317ef0"
      }
    },
    {
      "name": "Storage",
      "scope": "storage.type, storage.modifier, keyword.control",
      "settings": {
        // "fontStyle": "italic",
        "foreground": "#e4e4e4"
      }
    },
    {
      "name": "Async",
      "scope": "storage.modifier.async",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bdbbbb"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "#bdbbbb"
      }
    },
    {
      "name": "Type Annotation",
      "scope": "entity.name.type.instance.jsdoc,  meta.type.annotation.ts, storage.type.class.jsdoc",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5b777c"
      }
    },
    {
      "name": "Type Annotation Punctuation",
      "scope": "punctuation.definition.block.tag.jsdoc, punctuation.definition.bracket.curly.begin.jsdoc, punctuation.definition.bracket.curly.end.jsdoc",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#77a8b1"
      }
    },
    {
      "name": "Class variable",
      "scope": "variable.object.property, meta.field.declaration", // entity.name.function",
      "settings": {
        "foreground": "#f5f5f5"
      }
    },
    {
      "name": "Function definition", // object properties that are functions
      "scope": "meta.function", // entity.name.function",
      "settings": {
        // "foreground": "#5c8ac2"
        // "fontStyle": "bold",
        "foreground": "#6ebbbb"
      }
    },
    {
      "name": "Template expression",
      "scope": "template.expression.begin, template.expression.end, punctuation.definition.template-expression.begin, punctuation.definition.template-expression.end",
      "settings": {
        "foreground": "#b5ffef"
      }
    },
    {
      "name": "Reset embedded/template expression colors",
      "scope": "meta.embedded, source.groovy.embedded, meta.template.expression",
      "settings": {
        "foreground": "#F4F6F6"
      }
    },
    {
      "name": "YAML key",
      "scope": "entity.name.tag.yaml",
      "settings": {
        "foreground": "#c4ffff"
      }
    },

    //////
    //
    //
    //
    //           Golang
    //
    //
    //////

    //entity.name.function.go
    {
      "name": "go function name",
      "scope": "entity.name.function.go",
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#2a52d6"
      }
    },
    {
      "name": "Go Keyword",
      "scope": "keyword.function.go",
      "settings": {
        "fontStyle": "bold italic",
        "foreground": "#dbdada"
      }
    }, //storage.type.error.go

    //////
    //
    //
    //
    //           JSON & CSS
    //
    //
    //////
    {
      "name": "JSON key",
      "scope": "meta.object-literal.key, meta.object-literal.key string, support.type.property-name.json, variable.other.property",
      "settings": {
        "foreground": "#cacc52"
        // "foreground": "#aacc7e"
      }
    },
    {
      "name": "JSON constant",
      "scope": "constant.language.json",
      "settings": {
        "foreground": "#83ADD5"
      }
    },
    {
      "name": "CSS class",
      "scope": "entity.other.attribute-name.class",
      "settings": {
        "foreground": "#83ADD5"
      }
    },
    {
      "name": "CSS ID",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#F5C829"
      }
    },
    {
      "name": "CSS tag",
      "scope": "source.css entity.name.tag",
      "settings": {
        "foreground": "#3B73B7"
      }
    },
    {
      "name": "HTML tag outer",
      "scope": "meta.tag, punctuation.definition.tag",
      "settings": {
        "foreground": "#83ADD5"
      }
    },
    {
      "name": "HTML tag inner",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#c4ffff"
      }
    },
    {
      "name": "HTML tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#2438EA"
      }
    },
    {
      "name": "Markdown heading",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#83ADD5"
      }
    },
    {
      "name": "Markdown link text",
      "scope": "text.html.markdown meta.link.inline, meta.link.reference",
      "settings": {
        "foreground": "#c4ffff"
      }
    },
    {
      "name": "Markdown list item",
      "scope": "text.html.markdown beginning.punctuation.definition.list",
      "settings": {
        "foreground": "#83ADD5"
      }
    },
    {
      "name": "Markdown italic",
      "scope": "markup.italic",
      "settings": {
        "foreground": "#c4ffff",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown bold",
      "scope": "markup.bold",
      "settings": {
        "foreground": "#c4ffff",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold italic",
      "scope": "markup.bold markup.italic, markup.italic markup.bold",
      "settings": {
        "foreground": "#c4ffff",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Markdown code block",
      "scope": "markup.fenced_code.block.markdown punctuation.definition.markdown",
      "settings": {
        "foreground": "#F5C829"
      }
    },
    {
      "name": "Markdown inline code",
      "scope": "markup.inline.raw.string.markdown",
      "settings": {
        "foreground": "#F5C829"
      }
    }
    // {
    // 	"name": "Global settings",
    // 	"settings": {
    // 		"background": "#131e31",
    // 		"foreground": "#F4F6F6"
    // 	}
    // }
  ]
}
